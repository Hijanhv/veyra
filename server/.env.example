# Service role key is required for the server (writes & reads)
SUPABASE_SERVICE_ROLE_KEY=

# AI (optional; enables AI endpoints and auto-rebalancing logic)
ANTHROPIC_API_KEY=

# Automated Rebalancing (optional)
ENABLE_AUTO_REBALANCING=false
# Wallet for on-chain rebalancing execution (must be authorized in vault)
STRATEGY_MANAGER_PRIVATE_KEY=
