{
  "name": "veyra-fastify-server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/server.ts",
    "build": "tsc",
    "postbuild": "cp -R src/abi dist/abi || true",
    "start": "node dist/server.js",
    "abis:sync": "node ./scripts/sync-abis.mjs",
    "contracts:build": "node ./scripts/refresh-contracts.mjs",
    "db:types": "sh -c 'supabase gen types typescript --schema public --project-id \"${SUPABASE_PROJECT_REF:-iyrqtchhttmpyopfvipl}\" > src/types/supabase.generated.ts'",
    "indexer:dev": "ponder dev",
    "indexer:start": "ponder start"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.11",
    "@ai-sdk/openai": "^2.0.23",
    "@fastify/cors": "^9.0.1",
    "@supabase/supabase-js": "^2.45.4",
    "ai": "^5.0.30",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "fastify": "^4.24.3",
    "hono": "^4.6.8",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "nodemon": "^3.0.1",
    "ponder": "^0.12.21",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
